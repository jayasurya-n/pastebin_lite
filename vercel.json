{
    "version": 2,
    "builds": [
        {
            "src": "api/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/api/app.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/app.js"
        },
        {
            "src": "/p/(.*)",
            "dest": "/api/app.js"
        }
    ]
}